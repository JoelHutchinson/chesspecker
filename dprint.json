{
	"lineWidth": 80,
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
		"jsxSingleQuote": true,
		"singleQuote": true,
		"trailingComma": "all",
		"useTabs": true
	},
	"includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json}"],
	"excludes": ["**/node_modules", "**/*-lock.json"],
	"plugins": [
		"https://plugins.dprint.dev/prettier-0.9.0.json@76c20538ea4231b3daa9b62059f90ec144200cab5477b7e95b730cbb3ed48957"
	]
}
